citHeader("To cite postuR in publications use:")

citEntry(
  entry    = "Misc",
  title    = "postuR: An R Package for Posture Classification and Activity Measures from Triaxial Accelerometers in ECG Patches",
  author   = "Lacey H Etzkorn",
  year     = "2020",
  url      = "https://github.com/etzkorn/postuR",
  textVersion = paste0(
    # Package Citation
    "Etzkorn, Lacey H (2020). ",
    "'postuR:  An R Package for Posture Classification and Activity Measures from Triaxial Accelerometers in ECG Patches'. ",
    "R package version ",
    packageVersion("postuR"),
    ". URL: https://github.com/etzkorn/postuR."
))

